{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaversion": 8,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"react-app",
		"prettier"
	],
	"extends": [
		"react-app",
		"prettier"
  ],
	"rules": {
		"brace-style": 0,
		"linebreak-style": 0,
		"no-console": 0,
		"no-undef": 0,
		"indent": [0, 2],
		"jsx-a11y/href-no-hash": 0,
		"react/jsx-closing-bracket-location": 0,
		"react/jsx-indent-props": [0, 2],
		"react/jsx-indent": [0, 2],
		"react/jsx-no-undef": 0,
		"semi": [2, "never"],
		"no-unused-vars": 0,
		"no-unused-expressions": 0,
		"eqeqeq": 0
	}
}
